<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="66dp"
    android:background="@color/white"
    xmlns:tool="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <CheckBox
        android:id="@+id/cb_select_InvoiceListVH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="15dp"
        android:background="@null"
        android:button="@null"
        android:drawableLeft="@drawable/cb_dayuan_selector"
        android:padding="2dp"/>

    <TextView
        android:id="@+id/tv_time_InvoiceListVH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/cb_select_InvoiceListVH"
        android:layout_marginBottom="37dp"
        android:layout_marginStart="10dp"
        app:layout_goneMarginStart="15dp"
        android:textColor="@color/text_999999"
        android:textSize="11sp"
        tool:text="2018.12.24"/>

    <TextView
        android:id="@+id/tv_order_sn_InvoiceListVH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/tv_time_InvoiceListVH"
        android:layout_marginTop="37dp"
        android:textColor="@color/text_color"
        android:textSize="12sp"
        tool:text="@string/order_number"/>

    <TextView
        android:id="@+id/tv_money_InvoiceListVH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_order_sn_InvoiceListVH"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="15dp"
        android:textColor="@color/text_color"
        android:textSize="12sp"
        tool:text="@string/money_unit_2"/>

</android.support.constraint.ConstraintLayout>